{
	"datasets": {
		"source": [{
			"data": {
				"name": "NSW",
				"type": "CSV",
				"path": "nsw.csv"
			}
		}],
		"target": [{
			"data": {
				"name": "VIC",
				"type": "CSV",
				"path": "vic.csv"
			}
		}]
	},
	"mappings": [{
		"source": "NSW.AreaName",
		"target": "VIC.Area",
		"operation": "COPY"
	}, {
		"source": "NSW.AreaCode",
		"target": "VIC.AreaCode",
		"operation": "COPY"
	}, {
		"source": "NSW.Population_Persons",
		"target": "VIC.Population_Persons",
		"operation": "COPY"
	}, {
		"source": "NSW.Population_No_of_households",
		"target": "VIC.Total_Households",
		"operation": "COPY"
	}, {
		"source": "NSW.Population_Average_household_size",
		"target": "VIC.Av_People_Per_Household",
		"operation": "COPY"
	}]
}